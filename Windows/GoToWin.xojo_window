	#tag EndProperty


#tag EndWindowCode

#tag Events OKButton
	#tag Event
		Sub Action()
		  dim n as integer
		  n=val(GoToField.text)
		  if n>0 then
		    Parent.ExtractFragment(n-Genomewin.DisplayInterval/2,n+Genomewin.DisplayInterval/2)
		    'set the scrollbar:
		    Genomewin.HScrollBar.value=n
		    'Parent.featureleft=-1 'disable hit highlighting
		    hide
		  end if
		  
		  
		End Sub
	#tag EndEvent
