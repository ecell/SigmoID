		      'restore previous state:
		      select case CheckState
		      case "ViewLogo"
		        ViewLogo.Checked=true
		      case "ViewSequences"
		        ViewSequences.Checked=true
		      case "ViewAlignmentInfo"
		        ViewAlignmentInfo.checked=true
		      case "ViewHmmerSettings"
		        ViewHmmerSettings.Checked=true
		      case "ViewHmmProfile"
