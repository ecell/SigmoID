			Dim dlg as New SaveAsDialog
			Dim outfile as FolderItem
			'dlg.InitialDirectory=Profile_f
			'dlg.promptText="Prompt Text"
			
			dlg.SuggestedFileName=NthField(LogoFile.Name,".",1)+"_short.fasta"
