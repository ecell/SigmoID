		    if f.exists then
		      RepeatGenPath=f.ShellPath
		    else
		      msgbox "Can't find the RepeatGen.py script!"
		    end if
		  else
		    msgbox "A problem with the RepeatGen.py script (it's Nil!)"
		  end if
		  
		  f=resources_f.child("MastGen.py")
		  if f<>Nil then
		    if f.exists then
		      mastgenpath=f.ShellPath
		      'SettingsWin.mastGenPathField.text=mastgenpath
		    else
		      msgbox "Can't find the MastGen.py script"
		    end if
		  else
		    msgbox "Can't find the MastGen.py script"
		  end if
		  
		  'f=resources_f.child("Weblogo-3.3").child("weblogo")
		  'if f<>Nil then
		  'if f.exists then
		  '#if targetwin32
		  'weblogopath=pythonPath+f.ShellPath
